<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/launcherActivity">

    <activity
        android:id="@+id/launcherActivity"
        android:name="com.example.twitter.LauncherActivity"
        android:label="launcher"
        tools:layout="@layout/activity_launcher" />
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.twitter.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/welcome"
        android:name="com.example.twitter.WelcomeActivity"
        android:label="welcome"
        tools:layout="@layout/welcome_page" >
    </activity>
    <fragment
        android:id="@+id/createAccountFragment"
        android:name="com.example.twitter.CreateAccountFragment"
        android:label="fragment_create_account"
        tools:layout="@layout/activity_create_account" />

</navigation>